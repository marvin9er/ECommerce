<p id="notice"><%= notice %></p>

<h1>Armor Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Categoryid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @armor_categories.each do |armor_category| %>
      <tr>
        <td><%= armor_category.name %></td>
        <td><%= armor_category.categoryId %></td>
        <td><%= link_to 'Show', armor_category %></td>
        <td><%= link_to 'Edit', edit_armor_category_path(armor_category) %></td>
        <td><%= link_to 'Destroy', armor_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Armor Category', new_armor_category_path %>
